<Window x:Class="UI.ManagerWindows.CheckMasterPasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Master password" Height="300" Width="300"
        WindowStartupLocation="CenterScreen"
        Icon="../Icon.ico">
    <Grid>
        <Label Content="Enter the Master Password" Height="28" HorizontalAlignment="Left" Margin="69,25,0,0" Name="TitleLabel" VerticalAlignment="Top" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="49,184,0,0" Name="CancelButton" VerticalAlignment="Top" Width="75" Click="CancelButtonClick" IsCancel="True" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="156,184,0,0" Name="OKButton" VerticalAlignment="Top" Width="75" Click="OkButtonClick" IsDefault="True"/>
        <PasswordBox Height="45" HorizontalAlignment="Left" Margin="38,83,0,0" Name="PwTextbox" VerticalAlignment="Top" Width="206" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" PasswordChanged="PwTextboxPasswordChanged" />
    </Grid>
</Window>
